package ex_14;

import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		String[] palavras = new String[2];
		
		for (String palavra : palavras) {
			palavra = sc.nextLine();
		}
		
		
		
		sc.close();

	}

}
